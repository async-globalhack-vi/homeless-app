%h2 Login

= form_tag(action: 'create', method: 'post') do 
  %fieldset
    %p
      = label_tag :email, 'Email'
      = text_field_tag :email

    %p
      = label_tag :password, 'Password'
      = password_field_tag :password

  = submit_tag 'Login'

  = link_to 'Register', new_assistance_provider_path

